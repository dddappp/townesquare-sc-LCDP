<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"[]>
<hibernate-mapping package="xyz.townesquare.sc.domain.user">
  <class name="xyz.townesquare.sc.domain.user.AbstractUserState$SimpleUserState" table="user">
    <id name="userWallet" length="66">
      <column name="user_wallet" sql-type="VARCHAR(66)" />
      <generator class="assigned" />
    </id>
    <version name="offChainVersion" column="off_chain_version" type="long" />
    <property name="username">
      <column name="username" length="66" />
    </property>
    <property name="profileImage">
      <column name="profile_image" />
    </property>
    <property name="bio">
      <column name="bio" />
    </property>
    <property name="version">
      <column name="version" sql-type="DECIMAL(20,0)" />
    </property>
    <property name="createdBy">
      <column name="created_by" />
    </property>
    <property name="updatedBy">
      <column name="updated_by" />
    </property>
    <property name="active">
      <column name="active" />
    </property>
    <property name="deleted">
      <column name="deleted" />
    </property>
    <property name="createdAt" column="created_at" />
    <property name="updatedAt" column="updated_at" />
  </class>
</hibernate-mapping>
